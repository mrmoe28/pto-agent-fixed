{
  "name": "permit-search-landing",
  "version": "0.1.1",
  "private": true,
  "engines": {
    "node": ">=20.11 <=22",
    "npm": ">=10"
  },
  "optionalDependencies": {
    "@next/swc-darwin-arm64": "15.5.2",
    "@next/swc-darwin-x64": "15.5.2",
    "@next/swc-linux-x64-gnu": "15.5.2",
    "@next/swc-linux-x64-musl": "15.5.2",
    "@next/swc-win32-x64-msvc": "15.5.2"
  },
  "scripts": {
    "predev": "node scripts/check-env.js",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.cjs,.mjs --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run typecheck",
    "ci:test": "npm run check && npm run build && npm run qa:test",
    "qa:run": "npm run qa:test",
    "analyze": "ANALYZE=true next build",
    "process:scrape-jobs": "tsx scripts/process-scrape-jobs.ts",
    "migrate:db": "tsx scripts/run-migration.ts",
    "verify:db": "tsx scripts/verify-database.ts",
    "chrome:debug": "chmod +x scripts/launch-chrome-debug.sh && ./scripts/launch-chrome-debug.sh",
    "qa:audit": "tsx qa/run-qa-audit.ts",
    "qa:audit:headless": "tsx qa/run-qa-audit.ts --no-headless",
    "qa:test": "playwright test qa/audit-ui.spec.ts",
    "scrape:us:fast": "python -m cli crawl --urls data/seeds/us/start_urls.txt --depth 2 --out data/exports",
    "scrape:us:deep": "python -m cli crawl --urls data/seeds/us/start_urls.txt --depth 3 --allow-dynamic --out data/exports",
    "scrape:test:url": "python -m cli test-url --url",
    "db:optimize": "tsx scripts/apply-performance-indexes.ts",
    "db:analyze": "tsx scripts/analyze-performance.ts",
    "test:deep-crawl": "tsx scripts/test-deep-crawl.ts",
    "seed:ga-counties": "tsx scripts/seed-georgia-counties.ts"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.7.1",
    "@auth/prisma-adapter": "^2.8.0",
    "@googlemaps/js-api-loader": "^1.16.10",
    "@neondatabase/serverless": "^1.0.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@stripe/stripe-js": "^8.0.0",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^12.2.0",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "exceljs": "^4.4.0",
    "framer-motion": "^12.23.22",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.544.0",
    "next": "^15.5.2",
    "next-auth": "^5.0.0-beta.25",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "robots-parser": "^3.0.1",
    "stripe": "^19.0.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.2",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^20",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/xlsx": "^0.0.35",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "playwright": "^1.55.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5"
  }
}
